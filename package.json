{
  "name": "@sourceborn/agent-cli-sdk",
  "version": "0.1.0",
  "description": "TypeScript SDK for orchestrating AI-powered CLI tools (Claude Code, OpenAI Codex) in development workflows",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "test": "vitest",
    "test:console": "tsx examples/console-test.ts",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "check": "vitest run && npm run typecheck && npm run lint"
  },
  "keywords": [
    "ai",
    "claude",
    "codex",
    "openai",
    "cli",
    "automation",
    "development",
    "sdk",
    "typescript",
    "adapter",
    "workflow",
    "orchestration"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/sourceborn/agent-cli-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/sourceborn/agent-cli-sdk/issues"
  },
  "homepage": "https://github.com/sourceborn/agent-cli-sdk#readme",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "author": "Sourceborn",
  "license": "MIT",
  "engines": {
    "node": ">=22.0.0"
  },
  "dependencies": {
    "boxen": "^8.0.1",
    "chalk": "^5.3.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.57.0",
    "prettier": "^3.3.0",
    "tsx": "^4.7.0",
    "typescript": "^5.5.0",
    "vitest": "^2.0.0"
  }
}
